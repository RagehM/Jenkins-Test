pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git url: 'https://github.com/RagehM/Jenkins-Test.git', branch: 'master'
            }
        }
        stage('Build & Test') {
            steps {
                dir('Jenkins-Test') {
                    bat 'mvn clean package'
                }
            }
        }
    }
}